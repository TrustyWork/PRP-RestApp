{
  "name": "webpack2-bundle",
  "version": "2.0.0",
  "description": "Универсальный Webpack конфиг сделаный для обучения групп ProCode",
  "main": "index.js",
  "scripts": {
    "test": "echo \"All good, the world is still spinning\"",
    "devstart": "cross-env NODE_ENV=development webpack-dev-server --env.dev_serv=true --env.sw=true --env.proxy_host=localhost --env.proxy_port=3000 --env.port=4000",
    "start": "cross-env NODE_ENV=development webpack-dev-server --env.dev_serv=true --env.sw=true --env.proxy_host=localhost",
    "start_prod": "cross-env NODE_ENV=production webpack-dev-server --env.proxy_host=localhost",
    "build": "rimraf build && cross-env NODE_ENV=development webpack --env.sw=true --env.output_dir=./build",
    "dist": "rimraf dist && cross-env NODE_ENV=production webpack --env.sw=true --env.output_dir=./dist",
    "deploy": "cross-env NODE_ENV=production webpack --env.sw=true --env.output_dir=../public",
    "deploy_build": "cross-env NODE_ENV=development webpack --env.sw=true --env.output_dir=../public",
    "deploy_watch": "cross-env NODE_ENV=development webpack --env.sw=true --env.output_dir=../public --env.watch=true"
  },
  "keywords": [],
  "author": "Alexander Gavrilyuk",
  "license": "ISC",
  "devDependencies": {
    "babel-core": "^6.24.1",
    "babel-loader": "^7.0.0",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-preset-env": "^1.4.0",
    "babel-preset-react": "^6.24.1",
    "cross-env": "^4.0.0",
    "css-loader": "^0.28.1",
    "favicons-webpack-plugin": "0.0.7",
    "file-loader": "^0.11.1",
    "html-loader": "^0.4.5",
    "html-webpack-plugin": "^2.28.0",
    "node-sass": "^4.5.2",
    "postcss-loader": "^2.0.5",
    "preload-webpack-plugin": "^1.2.2",
    "react-hot-loader": "^3.0.0-beta.6",
    "rimraf": "^2.6.1",
    "sass-loader": "^6.0.5",
    "style-loader": "^0.17.0",
    "sw-precache-webpack-plugin": "^0.11.0",
    "url-loader": "^0.5.8",
    "webpack": "^2.5.1",
    "webpack-dev-server": "^2.4.5"
  },
  "dependencies": {
    "font-awesome": "^4.7.0",
    "material-ui": "^0.18.1",
    "normalize.css": "^7.0.0",
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "react-redux": "^5.0.5",
    "react-tap-event-plugin": "^2.0.1",
    "redux": "^3.6.0",
    "redux-form": "^6.7.0",
    "redux-thunk": "^2.2.0",
    "socket.io": "^2.0.1"
  }
}
